{
  "extends": "airbnb",
  "rules": {
    "indent": [
      2,
      4
    ],
    "react/jsx-indent": [
      2,
      4
    ],
    "react/jsx-indent-props": [
      2,
      4
    ],
    "linebreak-style": 0,
    "no-undef": 0,
    "max-len": 0,
    "no-use-before-define": 0,
    "no-underscore-dangle": 0,
    "strict": 0,
    "no-mixed-operators": 0
  }
}